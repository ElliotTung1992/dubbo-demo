<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="locations">
        <list>
            <value>ioc/part13/spiderMan.properties</value>
        </list>
        </property>
    </bean>

    <bean class="org.springframework.beans.factory.config.PropertyOverrideConfigurer">
        <property name="location" value="ioc/part13/spiderMan2.properties"/>
    </bean>

    <bean id="spiderMan" class="com.dge1992.spring.ioc.part13.SpiderMan">
        <property name="name">
            <value>${spiderMan.name}</value>
        </property>
        <property name="age">
            <value>${spiderMan.age}</value>
        </property>
    </bean>
</beans>