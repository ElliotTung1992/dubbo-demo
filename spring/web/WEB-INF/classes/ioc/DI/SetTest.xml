<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="dge1992.spring.bean.Person">
        <property name="name" value="成宝拉"/>
        <property name="age" value="11"/>
        <property name="sex" value="1"/>
    </bean>

    <bean id="person2" class="dge1992.spring.bean.Person">
        <property name="cars">
            <list>
                <bean class="dge1992.spring.bean.Car"></bean>
                <bean class="dge1992.spring.bean.Car"></bean>
            </list>
        </property>
        <property name="strs">
            <list>
                <value>111</value>
                <value>222</value>
            </list>
        </property>
    </bean>

    <bean id="person3" class="dge1992.spring.bean.Person">
        <property name="setValues">
            <set>
                <value>111</value>
                <ref bean="person"></ref>
            </set>
        </property>
    </bean>

    <bean id="person4" class="dge1992.spring.bean.Person">
        <property name="mapping">
            <map>
                <entry key="name">
                    <value>dong</value>
                </entry>
                <entry key="person">
                    <ref bean="person"/>
                </entry>
            </map>
        </property>
    </bean>

    <bean id="person5" class="dge1992.spring.bean.Person">
        <property name="car">
            <null/>
        </property>
    </bean>

</beans>