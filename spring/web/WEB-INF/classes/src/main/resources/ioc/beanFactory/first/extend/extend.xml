<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="cat" class="dge1992.spring.ioc.beanFactory.first.extend.Cat">
        <property name="name" value="cat"/>
    </bean>

    <bean id="redCat" class="dge1992.spring.ioc.beanFactory.first.extend.RedCat" parent="cat">
        <property name="colour" value="red"/>
    </bean>

    <bean id="blackCat" class="dge1992.spring.ioc.beanFactory.first.extend.BlackCat" parent="cat">
        <property name="colour" value="black"/>
    </bean>
</beans>